{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "npm install",
  "framework": "vite",
  "github": {
    "silent": true
  },
  "rewrites": [
    { "source": "/docs", "destination": "https://fataplus-docs.vercel.app" },
    { "source": "/docs/(.*)", "destination": "https://fataplus-docs.vercel.app/$1" }
  ]
}
