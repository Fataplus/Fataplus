# Example environment variables for Suna
# Copy this file to .env and fill values before deploying

PORT=3000
NODE_ENV=production
DATABASE_URL=postgres://user:password@hostname:5432/suna_db
OPENAI_API_KEY=
SECRET_KEY_BASE=change_me_generate_secure_random
REDIS_URL=redis://localhost:6379
CLOUDRON_APP_STORAGE=/app/data
LOG_LEVEL=info
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_USER=
EMAIL_PASSWORD=

# Optional: Sentry DSN for error tracking
SENTRY_DSN=

# Feature flags / optional settings
FEATURE_X_ENABLED=false